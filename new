#include <stdio.h>

void main()
{
	int c1, c2, c3;
	printf("Enter two number : ");
	scanf("%d%d",&c1,&c2);
	if(c1>c2)
	{
		int=c1;
	}
	else
	{
		int=c2;
	}
	printf("Biggest of the two number is %d\n");
}
