#include <stdio.h>
#include <math.h>

int main()
{
    float c1, c2, c3;
    printf("Enter c1 -> ");
    scanf("%f", &c1);
    printf("Enter c2 -> ");
    scanf("%f", &c2);
    printf("Enter c3 -> ");
    scanf("%f", &c3);
}






