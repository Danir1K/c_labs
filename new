#include <stdio.h>

void main()
{
	int c1, c2, c3;
	printf("Enter two number : ");
	scanf("%d%d",&c1,&c2);
	if(c1>c2)
	{
		int=c1;
	}
	else
	{
		int=c2;
	}
	printf("Biggest of the two number is %d\n");
}






















































#include <stdio.h>
int main()
{
float c1, c2, c3;
printf("Введите значение c1 ->\t");
scanf("%f", &c1);
printf("Введите значение c2 ->\t");
scanf("%f", &c2);
printf("Введите значение c3 ->\t");
scanf("%f", &c3);
int min;
    int number;
    if (c1 < c2){
        min = c1;
        number = 1;
    }
    if  (c2 < c1)
    {
        min = c2;
        number = 2;
    }    
    if (min > c3){
        min = c3;
        number = 3;
    }
int max;
int number2;
    if (c1 > c2){
        max = c1;
        number2 = 1;
    }
    if  (c1 < c2){
        max = c2;
        number2 = 2;
        }
    if (max < c3){
        max = c3;
        number2 = 3;
        }
float sum = c1+c2+c3;
float t;
if (max%2 == 0)
{
   t = ((max/sum)*100);
   printf("number = %d\n", number2);
   printf("%.1f\n", t);
}
else 
{
   t = (max - min);
   printf("number min = %d number max = %d\n", number, number2);
   printf("%.1f\n", t);
}
return 0;
}











